{
    "env": {
        "browser": true,
        "es2021": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
        "no-undef": "warn"
    },
    "globals": {
        "marked": "readonly",
        "feather": "readonly",
        "Chart": "readonly",
        "showToast": "readonly",
        "appData": "writable",
        "currentDayIndex": "writable",
        "config": "readonly",
        "taskUtils": "readonly"
    }
}
